{
  "hosting": {
    "headers": [ {
      "source" : "/*.js",
      "headers" : [ {
        "key" : "Content-Type",
        "value" : "text/javascript"
      } ]
    } ],
    "public": "Frontend/dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
